{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "shakticore-photo-restore",
  "version": "0.1.0",
  "identifier": "com.vipul.shakticore-photo-restore",
  "build": {
    "frontendDist": "../src",
    "beforeBuildCommand": "powershell -Command \"if (Test-Path '../models') { Copy-Item -Recurse -Force '../models' './models' }\""
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "shakticore-photo-restore",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "models/fast.onnx",
      "models/quality.onnx"
    ],
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}